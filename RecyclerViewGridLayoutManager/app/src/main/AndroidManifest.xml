<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.journaldev.recyclerviewgridlayoutmanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ArabicActivity">
            <intent-filter>
                <action android:name="com.journaldev.recyclerviewgridlayoutmanager.ArabicActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".BangleStyle" />
        <activity android:name=".Dulhan" />
        <activity android:name=".LaceGlove" />
        <activity android:name=".BackSide" />
        <activity android:name=".Feet"></activity>
    </application>

</manifest>